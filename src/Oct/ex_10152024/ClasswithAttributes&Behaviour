public class Dog {

    // Attributes
    String name;
    String Breed;
    int age;
    String color;

    // Constructor
    public Dog(String name, String Breed, int age, String color) {
        this.name = name;
        this.Breed = Breed;
        this.age = age;
        this.color = color;
    }

    // Behavior
    public void play() {
        System.out.println(name + " is playing");
    }

    public void eat() {
        System.out.println(name + " is eating");
    }

    public void bark() {
        System.out.println(name + " is barking");
    }

    public static void main(String args[]) {
        Dog dog = new Dog("rokith", "lab", 3, "white");
        Dog dog1 = new Dog("nithish", "lab", 4, "black");

        dog.eat();
        dog1.bark();
    }
}
